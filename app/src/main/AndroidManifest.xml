<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.geekfindlove">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GeekFindLove">

        <activity
            android:name=".AdminActivity1"
            android:label="@string/title_activity_admin1"
            android:theme="@style/Theme.GeekFindLove.NoActionBar"></activity>
        <activity
            android:name=".UserActivity1"
            android:label="@string/title_activity_user1"
            android:theme="@style/Theme.GeekFindLove.NoActionBar" />
        <activity android:name=".AdminActivity" />
        <activity android:name=".FirstTimeLogin" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".dummy.DummyActivity"></activity>
        <receiver android:name=".Reminder"/>
    </application>

</manifest>